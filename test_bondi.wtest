import Bondi_presentar.*

test "Pasajeros Aleatorios" {
    const Cole = new Bondi(Paradas = 20, Max_Pasajeros = 30)

    Cole.Comenzar()

    Cole.Avanzar()

    Cole.Avanzar()

    Cole.Avanzar_final()

    Cole.Recargar_Combustible()

    Cole.Avanzar()

    Cole.Avanzar()

    Cole.Avanzar_final()

    Cole.Recargar_Combustible()

    Cole.Avanzar_final()

    Cole.Recargar_Combustible()


}

